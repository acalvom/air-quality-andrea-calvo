<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="es.upm.miw.airquality">

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/air_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/air_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="es.upm.miw.airquality.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="es.upm.miw.airquality.LocationsActivity"
            android:label="@string/activity_locations"
            />
        <activity android:name="es.upm.miw.airquality.LocationDetail"
            android:label="@string/activity_location_detail"
            />
    </application>

</manifest>